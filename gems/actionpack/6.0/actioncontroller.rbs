    def key?: () -> bool
    def has_key?: () -> bool
    def values: () -> Array[untyped]
    def has_value?: () -> bool
    def empty?: () -> bool
    def include?: (untyped) -> bool
    def as_json: () -> String
    def to_s: () -> String
