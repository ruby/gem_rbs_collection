module ApmTraceable
  interface _Name
    def name: () -> String
  end

  interface _Trace
    def trace: [R] (String | Symbol trace_name, context_class: _Name?, **untyped _options) ?{ (?) -> R } -> R
  end

  def self.configure: [R] () { (Configuration configuration) -> R } -> R

  def self.configuration: () -> Configuration
end
