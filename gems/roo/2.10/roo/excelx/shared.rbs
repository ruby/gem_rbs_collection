module Roo
  class Excelx
    # Public:  Shared class for allowing sheets to share data. This should
    #          reduce memory usage and reduce the number of objects being passed
    #          to various inititializers.
    class Shared
      @dir: untyped

      @comments_files: untyped

      @sheet_files: untyped

      @rels_files: untyped

      @options: untyped

      @image_rels: untyped

      @image_files: untyped

      @styles: untyped

      @shared_strings: untyped

      @workbook: untyped

      attr_accessor comments_files: untyped

      attr_accessor sheet_files: untyped

      attr_accessor rels_files: untyped

      attr_accessor image_rels: untyped

      attr_accessor image_files: untyped

      def initialize: (untyped dir, ?::Hash[untyped, untyped] options) -> void

      def styles: () -> untyped

      def shared_strings: () -> untyped

      def workbook: () -> untyped

      def base_date: () -> untyped

      def base_timestamp: () -> untyped
    end
  end
end
