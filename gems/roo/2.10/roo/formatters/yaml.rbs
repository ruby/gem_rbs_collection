module Roo
  module Formatters
    module YAML
      # returns a rectangular area (default: all cells) as yaml-output
      # you can add additional attributes with the prefix parameter like:
      # oo.to_yaml({"file"=>"flightdata_2007-06-26", "sheet" => "1"})
      def to_yaml: (?::Hash[untyped, untyped] prefix, ?untyped? from_row, ?untyped? from_column, ?untyped? to_row, ?untyped? to_column, ?untyped sheet) -> ("" | untyped)
    end
  end
end
